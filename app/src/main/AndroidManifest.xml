<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="labs.dadm.l0302_widgets">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="labs.dadm.l0302_widgets.WidgetsActivity" />
        <activity android:name="labs.dadm.l0302_widgets.ButtonsActivity" />
        <activity
            android:name="labs.dadm.l0302_widgets.TextFieldsActivity"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name="labs.dadm.l0302_widgets.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>